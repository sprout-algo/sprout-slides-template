\definecolor{SproutBlack}{HTML}{000000}
\definecolor{SproutWhite}{HTML}{ffffff}
\definecolor{SproutDarkGray}{HTML}{44546a}
\definecolor{SproutLightGray}{HTML}{e7e6e6}
\definecolor{SproutLightBlue}{HTML}{5b9bd5}
\definecolor{SproutOrange}{HTML}{ed7d31}
\definecolor{SproutGray}{HTML}{a5a5a5}
\definecolor{SproutYellow}{HTML}{ffc000}
\definecolor{SproutDarkBlue}{HTML}{4472c4}
\definecolor{SproutGreen}{HTML}{70ad47}

\definecolor{SproutTitle}{HTML}{fc6e04}
\definecolor{SproutSubtitle}{HTML}{ffc000}
\definecolor{SproutFrameTitle}{HTML}{1f3864}
\definecolor{SproutText}{HTML}{385623}

\colorlet{SproutLinkColor}{SproutLightBlue}
\colorlet{SproutURLColor}{SproutLightBlue}

\setbeamercolor*{title}{fg=SproutTitle}
\setbeamercolor*{author}{fg=SproutSubtitle}
\setbeamercolor*{date}{fg=SproutSubtitle}
\setbeamerfont{title}{size=\Huge}
\setbeamerfont{author}{size=\Large}
\setbeamercolor{frametitle}{fg=SproutFrameTitle}
\setbeamercolor*{normal text}{fg=SproutText}

\setbeamertemplate{itemize items}[circle]
\setbeamertemplate{enumerate items}[default]

\setbeamercolor{itemize item}{fg=SproutText}
\setbeamercolor{itemize subitem}{fg=SproutText}
\setbeamercolor{itemize subsubitem}{fg=SproutText}
\setbeamercolor{enumerate item}{fg=SproutText}
\setbeamercolor{enumerate subitem}{fg=SproutText}
\setbeamercolor{enumerate subsubitem}{fg=SproutText}

\setbeamersize{text margin left=1.3cm,text margin right=1.3cm} 

\setbeamercolor{block body}{bg=SproutLightGray}
\setbeamercolor{block title}{bg=SproutGreen, fg=SproutWhite}
\colorlet{SproutTheoremTitleBg}{SproutDarkBlue}
\colorlet{SproutProofTitleBg}{SproutOrange}
\colorlet{SproutProblemTitleBg}{SproutLightBlue}

\renewcommand{\qedsymbol}{\color{SproutText}{\openbox}}
